        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        <title>{% block title %}Johan LOPES - Chef de projet Web - Expert Symfony - Lyon{% endblock %}</title>

        {% block stylesheets %}
            {% stylesheets
                '@JohanlopesCoreBundle/Resources/public/css/main.css'
                '@JohanlopesCoreBundle/Resources/public/fancybox/jquery.fancybox-1.3.4.css'
                '@JohanlopesCoreBundle/Resources/public/css/flexslider.css'
                '@JohanlopesCoreBundle/Resources/public/css/fontello.css'
                output='css/main.css'
                filter='yui_css' %}
                <link rel="stylesheet" href="{{ asset_url }}" type="text/css" media="all" />
            {% endstylesheets %}

            <!--[if IE 7]>
                <link rel="stylesheet" href="{{ asset('bundles/johanlopescore/css/fontello-ie7.css') }}">
            <![endif]-->
            <link rel="stylesheet" type="text/css" href="http://fonts.googleapis.com/css?family=Open+Sans:400,600,300,800,700,400italic|PT+Serif:400,400italic"/>

            <!--[if lt IE 9]>
