                <style>
                /* For IE < 8 (trigger hasLayout) */
                .clearfix {
                    zoom:1;
                }
                </style>
            <![endif]-->
        {% endblock stylesheets %}
    </head>

    <body>
        <!--Wrapper-->
        <div id="wrapper">
            {% include 'JohanlopesCoreBundle::header.html.twig' %}

            <!--Main content-->
            <div id="mainContent" class="clearfix">
                {% include 'JohanlopesCoreBundle::slider.html.twig' %}

                <!--Inner Content-->
                <div id="innerContent" class="clearfix">
                    {% block content %}{% endblock content %}

                    {% include 'JohanlopesCoreBundle::footer.html.twig' %}
                </div>
                <!--End inner Content-->
            </div>
            <!--End main content-->
        </div>
        <!--End wrapper-->

        <!--Javascript-->
        {% block javascripts %}
            <script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=false"></script>

            {% javascripts
                '@JohanlopesCoreBundle/Resources/public/js/jquery-1.9.1.min.js'
                '@JohanlopesCoreBundle/Resources/public/js/jquery-migrate-1.2.1.js'
                '@JohanlopesCoreBundle/Resources/public/js/jquery.quicksand.js'
                '@JohanlopesCoreBundle/Resources/public/js/jquery.easing.1.3.js'
                '@JohanlopesCoreBundle/Resources/public/js/jquery.flexslider.js'
                '@JohanlopesCoreBundle/Resources/public/js/jquery.fancybox-1.3.4.js'
                '@JohanlopesCoreBundle/Resources/public/js/jquery.mousewheel-3.0.4.pack.js'
                '@JohanlopesCoreBundle/Resources/public/js/slides.min.jquery.js'
                '@JohanlopesCoreBundle/Resources/public/js/jquery.scrollTo-min.js'
                '@JohanlopesCoreBundle/Resources/public/js/waypoints.js'
                '@JohanlopesCoreBundle/Resources/public/js/jquery.tweet.js'
                '@JohanlopesCoreBundle/Resources/public/js/jquery.gmap.min.js'
                '@JohanlopesCoreBundle/Resources/public/js/script.js'
                output='js/main.js'
                filter='yui_js' %}
                <script type="text/javascript" src="{{ asset_url }}"></script>
            {% endjavascripts %}

            <!-- Google analytics -->
            <script type="text/javascript">

              var _gaq = _gaq || [];
              _gaq.push(['_setAccount', 'UA-13192250-3']);
              _gaq.push(['_trackPageview']);

              (function() {
                var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
                ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
                var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
              })();

            </script>
            <!-- End google analytics -->

        {% endblock javascripts %}
